{
  "name": "sliffs",
  "version": "1.0.0",
  "description": "Prima789 LINE Member Card System with LIFF and Netlify Functions",
  "main": "index.js",
  "dependencies": {
    "@line/liff": "^2.27.2",
    "canvas": "^3.2.0",
    "node-fetch": "^3.3.2",
    "pg": "^8.11.3",
    "pg-cloudflare": "^1.2.7",
    "pg-connection-string": "^2.9.1",
    "pg-int8": "^1.0.1",
    "pg-pool": "^3.10.1",
    "pg-protocol": "^1.10.3",
    "pg-types": "^2.2.0",
    "pgpass": "^1.0.5",
    "postgres-array": "^2.0.0",
    "postgres-bytea": "^1.0.0",
    "postgres-date": "^1.0.7",
    "postgres-interval": "^1.2.0",
    "split2": "^4.2.0",
    "xtend": "^4.0.2"
  },
  "scripts": {
    "build": "echo 'Static files ready in public directory'",
    "dev": "netlify dev --node-version=20",
    "start": "netlify dev --node-version=20",
    "deploy": "netlify deploy --prod",
    "deploy:dev": "netlify deploy",
    "test": "echo \"Tests not implemented yet\" && exit 0",
    "check-env": "netlify env:list",
    "health": "curl ${NETLIFY_URL:-http://localhost:8888}/.netlify/functions/health-check",
